{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipDefaultLibCheck": true,
        "target": "ESNext",
        "noImplicitAny": false,
        "strict": true,
        "isolatedModules": true,
        "noEmit": true,
        "skipLibCheck": true,
        "moduleResolution": "Bundler",
        "strictNullChecks": false,
        "jsx": "react-jsx",
        "lib": ["es2022", "dom"],
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true
    }
}
